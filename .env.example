
# API key for cor-forum (set this in production)
COR_FORUM_API_KEY=your_api_key_here
# Database connection settings for cor-forum
COR_FORUM_DB_HOST=localhost
COR_FORUM_DB_PORT=3306
COR_FORUM_DB_NAME=corforum_database
COR_FORUM_DB_USER=corforum_user
COR_FORUM_DB_PASS=corforum_password

# Comma-separated list of allowed origins for CORS. Example:
# http://localhost:3000,https://example.com
CORS_ALLOWED_ORIGINS=http://localhost:3000
